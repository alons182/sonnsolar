		url('./fonts/4energy.ttf?-bodqv1') format('truetype'),
		url('./fonts/4energy.woff?-bodqv1') format('woff'),
		url('./fonts/4energy.svg?-bodqv1#4energy') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
